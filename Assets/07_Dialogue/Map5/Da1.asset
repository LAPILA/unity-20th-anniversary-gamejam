      buttonText_English: 
      nextDialogue: {fileID: 0}
      endingStackToAdd: 0
    text_Korean: Damn it! The ship is going to tear itself apart before we even reach
      the pod!
    text_English: Damn it! The ship is going to tear itself apart before we even
      reach the pod!
  - speakerName: Orion
    portrait: {fileID: 21300000, guid: 13bdbb6b2092c354b9ae018de9c52b17, type: 3}
