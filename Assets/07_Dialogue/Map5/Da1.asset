      nextDialogue: {fileID: 0}
      endingStackToAdd: 0
    text_Korean: Warning! Hull breach imminent! The time stabilization field can
      no longer compensate for the gravitational distortion!
    text_English: Warning! Hull breach imminent! The time stabilization field can
      no longer compensate for the gravitational distortion!
  - speakerName: LISA
    portrait: {fileID: 21300000, guid: 32ba029654ecb0a4a9fc7d7f092a65d9, type: 3}
